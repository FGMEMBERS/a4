<?xml version="1.0"?>
<PropertyList>

 <path>a4f-panel.ac</path>


  <animation>
    <type>rotate</type>
    <object-name>GearHandle</object-name>
    <object-name>GearHandleLight</object-name>
    <property>/controls/gear/gear-down</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>90</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.05</x-m>
      <y-m>-3.062</y-m>
      <z-m>0.011</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
   <type>pick</type>
   <object-name>GearHandle</object-name>
   <object-name>GearHandleLight</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
        <command>nasal</command>
        <script>controls.gearToggle();</script>
     </binding>
   </action>
 </animation>

  <animation>
    <type>material</type>
    <object-name>GearHandleLight</object-name>
    <emission>
		<red>1.0</red>
		<green>1.0</green>
		<blue>0.2</blue>
		<factor-prop>sim/alarms/gear</factor-prop>
	</emission>
  </animation>

   <animation>
    <type>rotate</type>
    <object-name>HookHandle</object-name>
    <property>/controls/gear/tailhook</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>90</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.05</x-m>
      <y-m>-3.062</y-m>
      <z-m>0.011</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
   <type>pick</type>
   <object-name>HookHandle</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
    <binding>
     <command>property-toggle</command>
     <property>controls/gear/tailhook</property>
   </binding>
   </action>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>Throttle</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
