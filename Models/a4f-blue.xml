  <!-- arrester hook -->
  <animation>
    <type>rotate</type>
    <object-name>Tailhook</object-name>
    <property>gear/tailhook/position-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>85</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>3.12</x-m>
      <y-m>0</y-m>
      <z-m>-0.70</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

