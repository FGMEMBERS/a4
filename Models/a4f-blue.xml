      <x-m>-0.44</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.54</z-m>
    </offsets>
  </model>

  <!-- Object Selection -->

<!--  <animation>
    <object-name>A4F Panel</object-name>  
    <object-name>Blackout</object-name>
  </animation>-->
	
	<animation>
    <type>select</type>
    <object-name>Panel</object-name>
    <condition>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>-1</value>
        </equals>
     </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Pilot</object-name>
    <condition>
      <not>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Fuselage</object-name>
    <condition>
      <not>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Empennage</object-name>
    <condition>
      <not>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>WingSurfaces</object-name>
    <condition>
      <not>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RightSlats</object-name>
    <object-name>LeftSlats</object-name>
    <condition>
      <equals>
        <property>controls/flight/slats</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>NoseGear</object-name>
    <object-name>NoseGearStrut</object-name>
    <object-name>NoseGearDoor</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LeftGear</object-name>
    <object-name>LeftGearStrut</object-name>
    <object-name>LeftGearDoor</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[1]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RightGear</object-name>
    <object-name>RightGearStrut</object-name>
    <object-name>RightGearDoor</object-name>
    <object-name>LandingLight</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[2]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RightFlap</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/flap-pos-norm</property>
        <value>1</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LeftFlap</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/flap-pos-norm</property>
        <value>1</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LeftAirBrake</object-name>
    <object-name>RightAirBrake</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/speedbrake-pos-norm</property>
        <value>0.001</value>
      </greater-than>
    </condition>
  </animation>

  <!-- Object Movement -->

  <animation>
    <type>rotate</type>
    <object-name>LeftGear</object-name>
    <property>gear/gear[1]/position-norm</property>
    <factor>-120</factor>
    <offset-deg>120</offset-deg>
    <min-deg>0</min-deg>
    <max-deg>90</max-deg>
    <center>
      <x-m>0.76</x-m>
      <y-m>-1.05</y-m>
      <z-m>-0.68</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftGear</object-name>
    <property>gear/gear[1]/position-norm</property>
    <factor>-120</factor>
    <offset-deg>120</offset-deg>
    <min-deg>0</min-deg>
    <max-deg>90</max-deg>
    <center>
      <x-m>0.76</x-m>
      <y-m>-1.05</y-m>
      <z-m>-0.68</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>LeftGearStrut</object-name>
    <property>gear/gear[1]/position-norm</property>
    <factor>45</factor>
    <offset-deg>-45</offset-deg>
