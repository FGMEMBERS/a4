<?xml version="1.0"?>
<PropertyList>

 <path>attitude.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/roll-deg</property>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/pitch-deg</property>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/heading-deg</property>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>

</PropertyList>
